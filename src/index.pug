- var page_title = "WISS 2020: 第28回インタラクティブシステムとソフトウェアに関するワークショップ"

doctype html
html(lang=ja)
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1.0, maximum-scale=1.0')
    include includes/seo
    title #{page_title}
    include includes/bootstrap-css
    link(rel="stylesheet" href="./css/override.css")
  body
    include includes/header
    .jumbotron
      .jumbotron-background-image
      .container
        h1 WISS 2020
        p.lead 第28回インタラクティブシステムとソフトウェアに関するワークショップ
    .container.content
      .alert.alert-primary 2020/09/04: <a href="./call-for-papers.html">論文募集ページ<a/>を掲載しました。
      include:markdown-it(html=true typographer=true) md/index.md
    include includes/footer
    include includes/bootstrap-js
